<h1>Forums !!<h3>
  <h2>Listing Forums</h2>

  <table>
    <thead>
      <tr>
        <th>Community</th>
        <th>Profile</th>
        <th>Title</th>
        <th>Body</th>
        <th>Create date</th>
        <th>Update date</th>
        <th>Type</th>
        <th>Priority</th>
        <th>Report</th>
        <th>Rating</th>
        <th>Ratecount</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= post.community_id %></td>
          <td><%= post.profile_id %></td>
          <td><%= post.title %></td>
          <td><%= post.body %></td>
          <td><%= post.create_date %></td>
          <td><%= post.update_date %></td>
          <td><%= post.data_type %></td>
          <td><%= post.priority %></td>
          <td><%= post.report %></td>
          <td><%= post.rating %></td>
          <td><%= post.rateCount %></td>
          <td><%= link_to 'Show', post %></td>
          <td><%= link_to 'Edit', edit_post_path(post) %></td>
          <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <a href="/newForum"> Start a Forum </a>
